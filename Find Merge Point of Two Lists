/*
   Find merge point of two linked lists
   Node is defined as
   struct Node
   {
       int data;
       Node* next;
   }
*/
int length(Node* head)
    {
    int count=0;
    for(Node* temp=head;temp!=NULL;temp=temp->next) count++;
    return count;
}
int FindMergeNode(Node *headA, Node *headB)
{
    // Complete this function
    // Do not write the main method. 
    int la=length(headA),lb=length(headB);
    if(la>lb)
     {
        int l=la-lb;
        while(l>0)
            {
            headA=headA->next;
            l--;
        }
    }
    else
        {
        int l=lb-la;
         while(l>0)
            {
            headB=headB->next;
            l--;
        }
    }
    while(1)
    {
        if(headA==headB) return headA->data;
        headA=headA->next;
        headB=headB->next;
    }
    
    
}

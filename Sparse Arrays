#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include<cstring>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int N;
    int box[N],j=0;
    cin>>N;
    string arr[N];
    for(int i=0;i<N;i++) cin>>arr[i];
    int Q,count=0;
    cin>>Q;
    string que[Q];
    for(int i=0;i<Q;i++) cin>>que[i];
    for(int j=0;j<Q;j++)
    {count=0;
        for(int i=0;i<N;i++)
        {
        if(arr[i]==que[j]) count++; 
    }
     cout<<count<<endl;
    }
     return 0;   
}
